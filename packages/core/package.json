{
  "name": "@standup/core",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./jira": {
      "import": "./dist/jira/index.mjs",
      "require": "./dist/jira/index.js",
      "types": "./dist/jira/index.d.ts"
    },
    "./claude": {
      "import": "./dist/claude/index.mjs",
      "require": "./dist/claude/index.js",
      "types": "./dist/claude/index.d.ts"
    },
    "./db": {
      "import": "./dist/db/index.mjs",
      "require": "./dist/db/index.js",
      "types": "./dist/db/index.d.ts"
    },
    "./encryption": {
      "import": "./dist/encryption.mjs",
      "require": "./dist/encryption.js",
      "types": "./dist/encryption.d.ts"
    },
    "./prompts": {
      "import": "./dist/prompts/index.mjs",
      "require": "./dist/prompts/index.js",
      "types": "./dist/prompts/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@supabase/supabase-js": "^2.47.12"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
