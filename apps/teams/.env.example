# Microsoft / Teams Bot Registration
# Get these from Azure Portal > Bot Services > Your Bot > Configuration
MICROSOFT_APP_ID=
MICROSOFT_APP_PASSWORD=
MICROSOFT_APP_TENANT_ID=

# Jira OAuth 2.0
# Create an OAuth 2.0 integration at https://developer.atlassian.com/console/myapps/
JIRA_CLIENT_ID=
JIRA_CLIENT_SECRET=
JIRA_REDIRECT_URI=https://your-app-url.vercel.app/api/auth/jira/callback

# Supabase
# Get from Supabase Dashboard > Settings > API
SUPABASE_URL=
SUPABASE_SERVICE_KEY=

# Anthropic (Claude AI)
# Get from https://console.anthropic.com/
ANTHROPIC_API_KEY=

# Token Encryption
# Generate with: openssl rand -hex 32
TOKEN_ENCRYPTION_KEY=

# App URL (for OAuth callbacks)
NEXT_PUBLIC_APP_URL=https://your-app-url.vercel.app
